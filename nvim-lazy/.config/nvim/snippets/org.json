{
  "Objective": {
    "prefix": "OB",
    "body": "*OBJECTIVE:* $0"
  },
  "Idea": {
    "prefix": "Id",
    "body": "*Idea:* /$1/$0"
  },
  "Visit": {
    "prefix": "Vi",
    "body": "*Visit:* $0"
  },
  "References": {
    "prefix": "Re",
    "body": "*Reference:* $0"
  },
  "Implemented": {
    "prefix": "Im",
    "body": "*Implemented:* $0"
  },
  "Checkout": {
    "prefix": "Ch",
    "body": "*Checkout:* $0"
  },
  "Keypoint": {
    "prefix": "Ke",
    "body": "*Keypoint:* $0"
  },
  "Workaround": {
    "prefix": "Wo",
    "body": "Workaround - $0"
  },
  "Solution": {
    "prefix": "So",
    "body": "*Solution:* $0"
  },
  "issues": {
    "prefix": "Is",
    "body": "*Issue:* $0"
  },
  "Checkpoint": {
    "prefix": "Cp",
    "body": "*Checkpoint:* $0"
  },
  "For more information": {
    "prefix": "FM",
    "body": "*FMI:* $0"
  },
  "Note": {
    "prefix": "No",
    "body": "*[NOTE]* taken on [$datetime]:\n  $0"
  },
  "Check-list": {
    "prefix": "Oc",
    "body": "- [ ] $0"
  },
  "Orgmode-hyperlink": {
    "prefix": "Li",
    "body": "[[${1:links/URLs}][${2:Description}]]$0"
  },
  "html-center-image": {
    "prefix": "Hia",
    "body": "#+name: ${4:title}\n#+begin_html html\n<p align='${2:center}'>\n<IMG src='${3:image}' alt='${4:title}' width=${5:50%}/>\n</p>\n#+end_html\n$0"
  }
}

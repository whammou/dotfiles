{
  "Objective": {
    "prefix": "OB",
    "body": "*OBJECTIVE:* $0"
  },
  "Idea": {
    "prefix": "Id",
    "body": "*Idea:* /$1/$0"
  },
  "Visit": {
    "prefix": "Vi",
    "body": "*Visit:* $0"
  },
  "References": {
    "prefix": "Re",
    "body": "*Reference:* $0"
  },
  "Implemented": {
    "prefix": "Im",
    "body": "*Implemented:* $0"
  },
  "Checkout": {
    "prefix": "Ch",
    "body": "*Checkout:* $0"
  },
  "Keypoint": {
    "prefix": "Ke",
    "body": "*Keypoint:* $0"
  },
  "Workaround": {
    "prefix": "Wo",
    "body": "Workaround - $0"
  },
  "Solution": {
    "prefix": "So",
    "body": "*Solution:* $0"
  },
  "issues": {
    "prefix": "Is",
    "body": "*Issue:* $0"
  },
  "Checkpoint": {
    "prefix": "Cp",
    "body": "*Checkpoint:* $0"
  },
  "For more information": {
    "prefix": "FM",
    "body": "*FMI:* $0"
  },
  "Note": {
    "prefix": "No",
    "body": "${1:-----\n}- *[NOTE]* taken on [$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE]: \\\\\\\n  $0\n${2:-----}"
  },
  "Recurring Tasks": {
    "prefix": "TS",
    "body": "${1:Title}\n- *OBJECTIVE:* $2\n- ${3:*Reference:*}$4\n-----\n#+NAME:LOGBOOK\n#+HTML:<details>\n#+HTML:<summary>LOGBOOK</summary>\n:LOGBOOK:\n:END:\n#+HTML:</details>\n-----\n$0"
  },
  "Check-list": {
    "prefix": "Oc",
    "body": "- [ ] $0"
  },
  "Orgmode-horizontal-line": {
    "prefix": "HL",
    "body": "-----"
  },
  "Orgmode-hyperlink": {
    "prefix": "Li",
    "body": "[[${1:links/URLs}][${2:Description}]]$0"
  },
  "Orgmode-schedule": {
    "prefix": "Sc",
    "body": "SCHEDULED: <$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE>"
  },
  "Orgmode-deadline": {
    "prefix": "Dl",
    "body": "DEADLINE: <$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE>"
  },
  "orgmode-quote-block": {
    "prefix": "quote",
    "body": "#+BEGIN_QUOTE\n$1\n#+END_QUOTE\n$0"
  },
  "zettelkasten-header": {
    "prefix": "zh",
    "body": [
      "#+TITLE: $1",
      "#+FILETAGS: :ZK:${2:Tags}:",
      "#+HTML:<details>\n",
      "* $1 :ZK:$2:",
      "#+HTML:</details>",
      "$0"
    ]
  },
  "doc-header": {
    "prefix": "dh",
    "body": [
      "#+FILETAGS: :DOC:META:${2:Tags}:",
      "#+HTML:<details>\n",
      "* $1 :DOC:META:$2:",
      "#+HTML:</details>",
      "$0"
    ]
  },
  "latex-inline-math": {
    "prefix": "mi",
    "body": "\\$$1\\$$0"
  },
  "github-special-block-note": {
    "prefix": "GN",
    "body": [
      "\n#+NAME:${1:Github Note Block}",
      "#+BEGIN_QUOTE markdown",
      "[!NOTE]",
      "$2",
      "#+END_QUOTE\n\n$0"
    ]
  },
  "github-special-block-warning": {
    "prefix": "GW",
    "body": [
      "\n#+NAME:${1:Github Warning Block}",
      "#+BEGIN_QUOTE markdown",
      "[!WARNING]",
      "$2",
      "#+END_QUOTE\n\n$0"
    ]
  },
  "github-special-block-important": {
    "prefix": "GI",
    "body": [
      "\n#+NAME:${1:Github Important Block}",
      "#+BEGIN_QUOTE markdown",
      "[!IMPORTANT]",
      "$2",
      "#+END_QUOTE\n\n$0"
    ]
  },
  "github-special-block-tip": {
    "prefix": "GT",
    "body": [
      "\n#+NAME:${1:Github tip Block}",
      "#+BEGIN_QUOTE markdown",
      "[!TIP]",
      "$2",
      "#+END_QUOTE\n\n$0"
    ]
  },
  "github-special-block-caution": {
    "prefix": "GC",
    "body": [
      "\n#+NAME:${1:Github caution Block}",
      "#+BEGIN_QUOTE markdown",
      "[!CAUTION]",
      "$2",
      "#+END_QUOTE\n\n$0"
    ]
  },
  "Github-hidden-section": {
    "prefix": "hcode",
    "body": [
      "#+NAME:${1:Details}",
      "#+html:<details>",
      "#+html:<summary><b>${1:Details}</b></summary>\n$2",
      "#+html:</details>\n$0"
    ]
  }
}
